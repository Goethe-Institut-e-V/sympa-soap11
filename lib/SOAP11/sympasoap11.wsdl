			</xsd:enumeration>
			<xsd:enumeration value="append">
				<xsd:annotation>
					<xsd:documentation>append to message body (append)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>	


