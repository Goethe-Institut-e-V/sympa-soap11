					<xsd:documentation>Email address of the subscriber.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="1" name="gecos" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Information about the subscriber (last name, first name, etc.)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="1" name="subscribed" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>is user subscribed</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- minOccurs was 0 => -->
			<xsd:element maxOccurs="1" minOccurs="1" name="date" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation/>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="1" name="update_date" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation/>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="1" name="reception" type="sym:SubscriberReceptionType">
				<xsd:annotation>
					<xsd:documentation>Special delivery modes which the subscriber may select.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="1" name="visibility" type="sym:UserVisibilityType">
				<xsd:annotation>
					<xsd:documentation>Special mode which allows the subscriber to remain invisible when a REVIEW command is issued for the list.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="1" name="number_messages" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation/>
