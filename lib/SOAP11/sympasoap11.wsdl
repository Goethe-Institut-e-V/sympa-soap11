			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="1" name="title" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Short description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>


    

