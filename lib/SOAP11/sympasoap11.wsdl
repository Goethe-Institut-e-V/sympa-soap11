				<xsd:annotation>
					<xsd:documentation/>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" name="inclusion" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>subscriber included?</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" name="inclusion_label" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>subscriber included?</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" name="inclusion_ext" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>subscriber included?</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" name="startdate" type="xsd:string">
