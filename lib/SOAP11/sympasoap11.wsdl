					<xsd:documentation>Short description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" name="visibility" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>is visible</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="subtopics" type="sym:SubTopicType">
				<xsd:annotation>
					<xsd:documentation>Subtopics</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>




	<xsd:complexType name="SubTopicType">
