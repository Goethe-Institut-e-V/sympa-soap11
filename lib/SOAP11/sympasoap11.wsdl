			<!-- <= minOccurs was 0 -->
			<!-- now optional elements -->
			<xsd:element maxOccurs="1" minOccurs="0" name="topics" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation/>
				</xsd:annotation>
			</xsd:element>
